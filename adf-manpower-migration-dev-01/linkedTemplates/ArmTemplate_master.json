{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-manpower-migration-dev-01"
		},
		"ls_azure_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://credentials-manpower-dev.vault.azure.net/"
		},
		"ls_migration_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-connection-sqldb-migration"
		},
		"ls_my_italy_new_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-connection-sqldb-my-italy-new"
		},
		"ls_raw_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-connection-sqldb-raw"
		},
		"triggerEG01_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ec78c6ac-1e72-4e40-8c1d-69dcb474e84d/resourceGroups/MPG-IT-Intiway-POC/providers/Microsoft.EventGrid/topics/pippevent1000"
		},
		"triggerEGB_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ec78c6ac-1e72-4e40-8c1d-69dcb474e84d/resourceGroups/MPG-IT-Intiway-POC/providers/Microsoft.EventGrid/topics/pippevent1000"
		},
		"triggerEGReceiver_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ec78c6ac-1e72-4e40-8c1d-69dcb474e84d/resourceGroups/MPG-IT-Intiway-POC/providers/Microsoft.EventGrid/topics/eventcompleted"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-manpower-migration-dev-01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_azure_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_migration_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_migration_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_my_italy_new_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_my_italy_new_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_raw_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_raw_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-manpower-migration-dev-01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"triggerEG01_properties_typeProperties_scope": {
						"value": "[parameters('triggerEG01_properties_typeProperties_scope')]"
					},
					"triggerEGB_properties_typeProperties_scope": {
						"value": "[parameters('triggerEGB_properties_typeProperties_scope')]"
					},
					"triggerEGReceiver_properties_typeProperties_scope": {
						"value": "[parameters('triggerEGReceiver_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-manpower-migration-dev-01_ArmTemplate_0')]"
			]
		}
	]
}